    "engines": {
        "node": ">=6.9.0"
    },
    "scripts": {
        "lint": "tslint src/**/*.ts --force",
        "lint:fix": "tslint src/**/*.ts --fix --force",
        "start": "yarn run clear && yarn run webpack:dev",
        "node:install": "node config/install.js",
        "clear": "node config/clear.js",
        "clear:prod": "node config/clear.js --p",
        "ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json",
        "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js dist/",
        "webpack:build": "webpack --config config/webpack.dev.js",
        "webpack:dev": "webpack-dev-server --config config/webpack.dev.js  --progress --inline --hot-only --hot --profile --port=9060",
        "webpack:prod": "yarn run node:install && yarn run clear:prod && node --max_old_space_size=3072 node_modules/webpack/bin/webpack.js -p --config config/webpack.prod.js --progress --profile"
    }
}
